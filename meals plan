<h2 id="mntl-sc-block_1-0-256">Day 7</h2>
<!-- /wp:heading -->

<!-- wp:image {"align":"center","id":255,"sizeSlug":"full","linkDestination":"none"} -->
<figure class="wp-block-image aligncenter size-full"><img src="/wp-content/uploads/2022/10/image-60.png" alt="" class="wp-image-255"/></figure>
<!-- /wp:image -->

<!-- wp:heading {"level":3} -->
<h3 id="mntl-sc-block_1-0-258">Breakfast</h3>
<!-- /wp:heading -->

<!-- wp:list -->
<ul id="mntl-sc-block_1-0-259"><li>1 cup cooked oatmeal</li><li>1/2 cup blueberries</li><li>1/2 cup non-fat milk</li><li>2 tablespoons almond butter<strong></strong></li></ul>
